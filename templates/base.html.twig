                        </div>
                        <div class="hidden tablet:block h-full w-full">
                            <div class="flex h-full">
                                <div class="w-full flex justify-center">
                                    <div class="space-x-4 flex flex-row ml-auto">
                                        <!-- Active: "bg-gray-900 text-white", Default: "text-gray-300 hover:bg-gray-700 hover:text-white" -->
                                        <a href="{{ path('home') }}" class="px-3 py-2 w-auto text-lg font-medium tracking-wide flex items-center text-black dark:text-white underline-offset-4 decoration-[3px] hover:underline decoration-sky-600" {{ menu_active('home') }}>
                                            Accueil
                                        </a>
                                        <a href="{{ path('shop_index') }}" class="px-3 py-2 text-lg font-medium tracking-wide flex items-center text-black dark:text-white underline-offset-4 decoration-[3px] hover:underline decoration-sky-600" {{ menu_active('shop') }}>
                                            Boutiques
                                        </a>
                                        <a href="{{ path('product_index') }}" class="px-3 py-2 text-lg font-medium tracking-wide flex items-center text-black dark:text-white underline-offset-4 decoration-[3px] hover:underline decoration-sky-600" {{ menu_active('product') }}>
                                            Produits
                                        </a>
                                        {% if app.user is defined and app.user is not null %}
                                            <a href="{{ path('basket_index') }}" class="px-3 py-2 text-lg font-medium tracking-wide flex items-center text-black dark:text-white underline-offset-4 decoration-[3px] hover:underline decoration-sky-600" {{ menu_active('basket') }}>
                                                Panier
                                            </a>
                                        {% endif %}
                                    </div>
                                    <div class="flex items-center ml-auto space-x-4">
                                        {% if app.user is defined and app.user is not null %}
                                            <div class="relative px-3 font-medium my-auto inline-block items-center">
                                                <div>
                                                    <button
                                                            type="button"
                                                            class="inline-flex justify-center ring-2 items-center w-full min-w-max rounded-lg shadow-lg px-2 py-1 font-bold
                                                                focus:outline-none ring-slate-200 dark:ring-sky-600 text-slate-700 dark:text-white hover:text-slate-900
                                                                dark:hover:text-slate-100 hover:ring-slate-300 dark:hover:ring-sky-500 active:text-slate-600
                                                                focus-visible:outline-sky-600 focus-visible:ring-slate-300 transition-all duration-500"
                                                            aria-expanded="true" aria-haspopup="true" onclick="openDropdown('menu-button')"
                                                    >
                                                        Mon compte
                                                        <!-- Heroicon name: solid/chevron-down -->
                                                        <svg class="h-5 w-5 -mr-0.5 ml-1 mt-[2.5px]" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                                                            <path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd" />
                                                        </svg>
                                                    </button>
                                                </div>
                                                <!-- Dropdown menu, show/hide based on menu state. -->
                                                <div class="origin-top absolute right-0 mt-2.5 w-42 text-black dark:text-white rounded-lg hidden shadow-lg dark:shadow-none bg-slate-100 z-40 dark:bg-slate-800 min-w-max w-full divide-y divide-gray-100 dark:divide-slate-700" role="menu" aria-orientation="vertical" id="menu-button" aria-labelledby="menu-button" tabindex="-1">
                                                    <div class="py-1" role="none">
                                                        <h6 class="px-4 py-2 block">Connecté en tant que <span class="text-sky-500 dark:text-sky-400 font-bold">{{ app.user.surname }} {{ app.user.name }}</span></h6>
                                                    </div>
                                                    {% if is_granted('ROLE_MERCHANT') %}
                                                        <div class="py-1" role="none">
                                                            <!-- Active: "bg-gray-100 text-gray-900", Not Active: "text-gray-700" -->
                                                            <a href="{{ path('seller_choose') }}" class="block px-4 py-2 underline-offset-4 decoration-[3px] hover:underline decoration-sky-600" role="menuitem" tabindex="-1" id="menu-item-0">Ma boutique</a>
                                                        </div>
                                                    {% endif %}
                                                    <div class="py-1" role="none">
                                                        <a href="{{ path('user_edit') }}" class="block px-4 py-2 underline-offset-4 decoration-[3px] hover:underline decoration-sky-600" role="menuitem" tabindex="-1" id="menu-item-1">Mon profil</a>
                                                        <a href="{{ path('user_orders') }}" class="block px-4 py-2 underline-offset-4 decoration-[3px] hover:underline decoration-sky-600" role="menuitem" tabindex="-1" id="menu-item-2">Mes commandes</a>
                                                    </div>
                                                    <div class="py-1" role="none">
                                                        <a href="{{ path('app_logout') }}" class="block px-4 py-2 underline-offset-4 decoration-[3px] hover:underline decoration-sky-600" role="menuitem" tabindex="-1" id="menu-item-3">Déconnexion</a>
                                                    </div>
                                                </div>
                                            </div>
                                        {% else %}
                                            <a href="{{ path('app_login') }}" class="flex items-center justify-center align-middle h-10 my-auto">
                                                <button
                                                        class="px-3.5 py-1 ring-2 rounded-lg font-bold focus:outline-none ring-slate-300
                                                            dark:ring-sky-600 text-slate-700 dark:text-white hover:text-slate-900
                                                            dark:hover:text-slate-100 hover:ring-slate-300 dark:hover:ring-sky-500
                                                            active:text-slate-600 focus-visible:outline-sky-600 focus-visible:ring-slate-300
                                                            transition-all duration-500"
                                                >
                                                    Connexion
                                                </button>
                                            </a>
                                            <a href="{{ path('app_signup') }}" class="flex items-center justify-center align-middle h-10 my-auto">
                                                <button
                                                        class="px-3.5 py-1.5 rounded-lg font-bold focus:outline-none focus-visible:outline-2
                                                            focus-visible:outline-offset-2 bg-slate-900 dark:bg-sky-600 text-white hover:bg-slate-700
                                                            dark:hover:bg-sky-500 hover:text-slate-100 focus-visible:outline-slate-900 transition-all
                                                            duration-500"
                                                >
                                                    Inscription
                                                </button>
                                            </a>
                                        {% endif %}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Mobile menu, show/hide based on menu state. -->
            <div class="hidden tablet:hidden relative px-6 z-50" id="mobile-menu">
                <div>
                    <div class="fixed inset-0 bg-slate-300/50 dark:bg-slate-800/50 opacity-100" aria-hidden="true"></div>
                    <ul class="absolute inset-x-3 top-full mt-1 flex origin-top flex-col gap-y-4 rounded-2xl bg-white dark:bg-slate-900 p-6 text-lg font-medium text-slate-900 dark:text-white shadow-xl dark:shadow-none ring-1 ring-slate-900/5 opacity-100 scale-100" tabindex="-1">
                        <li>
                            <a href="{{ path('home') }}" class="w-full underline-offset-4 decoration-[2px] hover:underline decoration-sky-600" {{ menu_active('home') }}>
                                Accueil
                            </a>
                        </li>
                        <li>
                            <a href="{{ path('shop_index') }}" class="w-full underline-offset-4 decoration-[2px] hover:underline decoration-sky-600" {{ menu_active('shop') }}>
                                Boutiques
                            </a>
                        </li>
                        <li>
                            <a href="{{ path('product_index') }}" class="w-full underline-offset-4 decoration-[2px] hover:underline decoration-sky-600" {{ menu_active('product') }}>
                                Produits
                            </a>
                        </li>
                        {% if app.user is defined and app.user is not null %}
                            <li>
                                <a href="{{ path('basket_index') }}" class="w-full underline-offset-4 decoration-[2px] hover:underline decoration-sky-600" {{ menu_active('basket') }}>
                                    Panier
                                </a>
                            </li>
                            <li class="border-t border-slate-300/40 dark:border-slate-600/40 pt-4">
                                <div class="relative px-3 font-medium my-auto inline-block items-center w-full">
                                    <div>
                                        <button type="button" aria-expanded="true" aria-haspopup="true" onclick="openDropdown('mobile-button')"
                                                class="inline-flex justify-center ring-2 items-center w-full min-w-max rounded-lg px-2 py-1 font-bold
                                                focus:outline-none ring-slate-200 dark:ring-sky-600 text-slate-700 dark:text-white hover:text-slate-900
                                                dark:hover:text-slate-100 hover:ring-slate-300 dark:hover:ring-sky-500 active:text-slate-600
                                                focus-visible:outline-sky-600 focus-visible:ring-slate-300 transition-all duration-500"
                                        >
                                            Mon compte
                                            <!-- Heroicon name: solid/chevron-down -->
                                            <svg class="h-5 w-5 -mr-0.5 ml-1 mt-[2.5px]" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                                                <path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd"/>
